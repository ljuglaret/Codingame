import java.util.*;
import java.io.*;
import java.math.*;




class Solution {


    public static BigInteger pgcd (int a , int b){
      
     BigInteger a1 = BigInteger.valueOf(a);
      BigInteger b1 = BigInteger.valueOf(b);
        
        return a1.gcd(b1);    
        
    }
    public static boolean premierAvec(int   a , int  n){
 
     return (pgcd(a,n).equals(BigInteger.valueOf(1)));
     
    }

    public static boolean isPrime(int x){
        boolean b = true;
        for (int i = 2 ; i< x ;  i++){
            if(!premierAvec(i,x)){
                b = false;
            }
        }
        return b;

    }
    
   
        
    
    
    public static boolean pseudoPremier(int num){
        if (isPrime(num)) {
            return false;
        }
        BigInteger numbi = BigInteger.valueOf(num);
        for (int i = 2; i < num; i++) {

            BigInteger modpow = BigInteger.valueOf(i).modPow(numbi, numbi);
            if (i == modpow.intValue()) {
            } else {
                return false;
            }
        }
        return true;
        }

    public static void main(String args[]) {
        Scanner in = new Scanner(System.in);
        int n = in.nextInt();
        if (pseudoPremier(n)){
            System.out.println("YES");    
        }
        else{
            System.out.println("NO");
        }
        // Write an action using System.out.println()
        // To debug: System.err.println("Debug messages...");

    }
}
